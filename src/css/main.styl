@import './common.css';
@import './hamburger.css';
@import './mixins.styl';
@import './slick.styl';

input[type="text"],
input[type="email"],
input[type="tel"],
textarea
  -webkit-appearance: none

::-webkit-input-placeholder
  color: #ffffff
::-moz-placeholder
  color: #ffffff

a
  display: inline-block
  text-decoration: none

img
  display: inline-block
  max-width 100%

ul
  list-style-type: none
  margin: 0
  padding: 0

button
  padding: 0
  border: 0
  cursor: pointer

